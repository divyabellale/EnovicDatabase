<div class="container">

<div class="row">
  <div class="col-sm-6">
<div class="row">
  <div class="col-sm-12">
      <h2 class="title-headings pull-left" style="">Fee Status list</h2>
  </div>
</div>
<table  class="table table-striped">
  <thead>
    <tr>
      <th>Amount paid</th>
      <th>Paid date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fee_statuses.each do |fee_status| %>
      <tr>
        <td><%= fee_status.amount_paid %></td>
        <td><%= fee_status.paid_date %></td>
        <td><%= link_to 'Show', fee_status %></td>
        <td><%= link_to 'Edit', edit_fee_status_path(fee_status) %></td>
        <td><%= link_to 'Destroy', fee_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<div class="col-sm-6">
<%= render 'form' %>
</div>
</div>
</div>



